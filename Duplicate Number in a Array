public class MyClass {
    public static void main(String args[]) {
    int arr [] = {1,2,2,4,3,4,6,7,8};
   int len = arr.length;
   for(int i = 0;i<len-1;i++){
       for(int j = i+1;j<=len-1;j++){
           if(arr[i]==arr[j]){
               System.out.println("Duplicate no in an array is : " + arr[j]);
                }
            }
        }
    }
}
