public class MyClass {
    public static void main(String args[]) {
      int[] arr = {1,2,3,5,7,80};
      System.out.println(max(arr));
    }
    static int max(int[] arr){
        int max = arr[0];
        for(int i = 0;i<arr.length;i++){
        if(max<arr[i]){
           max = arr[i];
        }
    }
    return max;
}
}
